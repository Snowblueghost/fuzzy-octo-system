<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.flappybird" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Poised Cherry Tappy plane</name>
    <content src="index.html" />
    <plugin name="cordova-plugin-screen-orientation" version="3.0.2" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="GradlePluginKotlinEnabled" value="true" />
        <preference name="loadUrlTimeoutValue" value="60000" />
        <icon density="ldpi" src="www/android-icon-36.png" />
        <icon density="mdpi" src="www/android-icon-48.png" />
        <icon density="hdpi" src="www/android-icon-72.png" />
        <icon density="xhdpi" src="www/android-icon-96.png" />
        <icon density="xxhdpi" src="www/android-icon-144.png" />
        <icon density="xxxhdpi" src="www/android-icon-192.png" />
        <preference name="AndroidWindowSplashScreenBackground" value="#000000" />
        <config-file parent="/manifest" target="AndroidManifest.xml">
            <queries>
                <intent>
                    <action android:name="android.support.customtabs.action.CustomTabsService" />
                </intent>
            </queries>
        </config-file>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="SwiftVersion" value="5.3" />
        <preference name="deployment-target" value="12.0" />
        <icon height="180" src="www/ios-icon-180.png" width="180" />
        <icon height="60" src="www/ios-icon-60.png" width="60" />
        <icon height="120" src="www/ios-icon-120.png" width="120" />
        <icon height="76" src="www/ios-icon-76.png" width="76" />
        <icon height="152" src="www/ios-icon-152.png" width="152" />
        <icon height="40" src="www/ios-icon-40.png" width="40" />
        <icon height="80" src="www/ios-icon-80.png" width="80" />
        <icon height="57" src="www/ios-icon-57.png" width="57" />
        <icon height="114" src="www/ios-icon-114.png" width="114" />
        <icon height="72" src="www/ios-icon-72.png" width="72" />
        <icon height="144" src="www/ios-icon-144.png" width="144" />
        <icon height="167" src="www/ios-icon-167.png" width="167" />
        <icon height="29" src="www/ios-icon-29.png" width="29" />
        <icon height="58" src="www/ios-icon-58.png" width="58" />
        <icon height="87" src="www/ios-icon-87.png" width="87" />
        <icon height="50" src="www/ios-icon-50.png" width="50" />
        <icon height="20" src="www/ios-icon-20.png" width="20" />
        <icon height="100" src="www/ios-icon-100.png" width="100" />
        <icon height="167" src="www/ios-icon-167.png" width="167" />
        <icon height="1024" src="www/ios-icon-1024.png" width="1024" />
    </platform>
    <preference name="AndroidXEnabled" value="true" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="Fullscreen" value="true" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <plugin name="cordova-plugin-inappbrowser">
    </plugin>
    <plugin name="cordova-plugin-safariviewcontroller">
    </plugin>
    <plugin name="cordova-plugin-ionic-webview" version="5.0.1" />
</widget>
